    "base_url": "https://7c73b6eff3a9.ngrok-free.app",
    "auth": {
        "type": "jwt"
    },
    "lifecycle_callback": {
        "install": "/install_cb",
        "enabled": "/enabled_cb",
        "disabled": "/disabled_cb",
        "uninstalled": "/uninstalled_cb"
    },
    "oauth": {
        "type": ["app", "user"],
        "redirect_uri": "https://7c73b6eff3a9.ngrok-free.app/oauth/callback",
