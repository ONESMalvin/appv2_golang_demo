{
    "id": "app_f63grnbjr6xinlyk",
    "name": "Malvin's Demo App",
    "version": "v1.0.2",
    "desc": "功能描述：\n1.禁止登记工时 \n2.客户端API调用返回信息展示\n3.团队信息展示\n",
    "base_url": "https://p8205-k3s-9.k3s-dev.myones.net/platform/plugin_relay/app_dispatch/app_f63grnbjr6xinlyk",
    "auth": {
        "type": "jwt"
    },
    "lifecycle_callback": {
        "install": "/install_cb",
        "enabled": "/enabled_cb",
        "disabled": "/disabled_cb",
        "uninstalled": "/uninstalled_cb"
    },
    "oauth": {
        "type": ["app", "user"],
        "scope": [
            "read:project:issue",
            "write:project:issue",
            "read:project:project",
            "read:account:teams",
            "read:account:user"
        ]
    },
    "extensions": {
        "manhourValidator": [
            {
                "key": "validator1",
                "funcs": {
                    "validate": "/manhour/validate"
                }
            }
        ],
        "appSettingPages": [
            {
                "key":"settingPage",
                "funcs": {
                    "customEntries": "/settingPage/entries"
                }
            }
        ]
    },
    "events": {
        "url": "/events/webhook",
        "types": [
            {
                "eventType": "ones:project:issue:created"
            },
            {
                "eventType": "ones:project:issue:updated"
            },
            {
                "eventType": "ones:project:project:created"
            },
            {
                "eventType": "ones:project:project:updated"
            }
        ]
    }
}
